BA4EA,SA5EA,EA4EA;AAAA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,eAAA,SAAA,EAAA,IAAA;AAAA,4EAAA,kBAAA,GAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBACA,GAAA,CAAA,IADA;;AAAA;AAEA,kBAAA,MAAA,CAAA,IAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAIA,KAjFA;AAmFA,IAAA,UAnFA,sBAmFA,QAnFA,EAmFA;AAAA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,cAAA,QAAA,EAAA,IAAA;AAAA,4EAAA,kBAAA,GAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBACA,GAAA,CAAA,IADA;;AAAA;AAEA,kBAAA,MAAA,CAAA,WAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAIA,KAxFA;AA0FA,IAAA,WA1FA,yBA0FA;AAAA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA,EAAA,IAAA;AAAA,4EAAA,kBAAA,GAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBACA,GAAA,CAAA,IADA;;AAAA;AACA,kBAAA,MAAA,CAAA,UADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAGA,KA9FA;AAgGA,IAAA,IAhGA,kBAgGA;AAAA;;AACA,MAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,WAAA,EAAA,IAAA;AAAA,4EAAA,kBAAA,GAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yBACA,GAAA,CAAA,IADA;;AAAA;AACA,kBAAA,MAAA,CAAA,WADA;AAEA,kBAAA,MAAA,CAAA,mBAAA,GAAA,KAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AAIA;AArGA,GAvDA;AA+JA,EAAA,OA/JA,qBA+JA;AACA,SAAA,IAAA;AACA,SAAA,WAAA;AACA,SAAA,mBAAA,GAAA,KAAA;AACA;AAnKA,CAAA","sourcesContent":["<template>\n  <div class=\"dashboard\">\n    <Navbar />\n    <div class=\"container\">\n      <h4 class=\"title is-4\">\n        Métricas de Qualidade para os cards do Desenvolvimento\n      </h4>\n\n      <button\n        class=\"button is-danger is-outlined is-rounded\"\n        @click=\"showProductAddModal = true\"\n      >\n        Novo Sistema</button\n      ><br /><br />\n\n      <div class=\"columns is-multiline\">\n        <div\n          class=\"column is-4\"\n          v-for=\"product in productList\"\n          :key=\"product._id\"\n        >\n          <div class=\"card\">\n            <div class=\"card-content\">\n              <div class=\"media\">\n                <div class=\"media-left\">\n                  <figure class=\"image is-48x48\">\n                    <img\n                      src=\"../assets/logo-fortes.gif\"\n                      alt=\"Logo Fortes Tecnologia\"\n                    />\n                  </figure>\n                </div>\n                <div class=\"media-content\">\n                  <p class=\"title is-4\">{{ product.name }}</p>\n                </div>\n              </div>\n            </div>\n            <footer class=\"card-footer\">\n              <a href=\"/formulario\" class=\"card-footer-item\">Inserir Nota</a>\n              <a\n                class=\"card-footer-item\"\n                @click=\"confirmCustomDelete(product._id)\"\n                >Apagar Sistema</a\n              >\n            </footer>\n          </div>\n        </div>\n      </div>\n\n      <b-modal\n        v-model=\"showProductAddModal\"\n        has-modal-card\n        trap-focus\n        :destroy-on-hide=\"false\"\n        aria-role=\"dialog\"\n        aria-modal\n      >\n        <form action=\"\">\n          <div class=\"modal-card\">\n            <header class=\"modal-card-head\">\n              <p class=\"modal-card-title\">Cadastrar Novo Sistema Fortes</p>\n            </header>\n            <section class=\"modal-card-body\">\n              <div class=\"field\">\n                <input\n                  class=\"input\"\n                  v-model=\"form.name\"\n                  placeholder=\"Nome do Sistema\"\n                />\n                <small class=\"has-text-danger\" v-if=\"errorName === true\"\n                  >Campo Obrigatório</small\n                >\n              </div>\n              <div class=\"field\">\n                <textarea\n                  class=\"textarea\"\n                  v-model=\"form.description\"\n                  placeholder=\"Descrição\"\n                ></textarea>\n              </div>\n            </section>\n            <footer class=\"modal-card-foot\">\n              <button\n                class=\"button is-danger is-outlined is-rounded\"\n                @click=\"closeModal\"\n              >\n                Cancelar\n              </button>\n              <button\n                type=\"button\"\n                class=\"button is-danger is-outlined is-rounded\"\n                @click=\"create\"\n              >\n                Cadastrar\n              </button>\n            </footer>\n          </div>\n        </form>\n      </b-modal>\n      <br /><br /><br /><br />\n      <div class=\"container\">\n        <nav class=\"level\">\n          <div class=\"level-right\">\n            <div class=\"level-item\">\n              <div class=\"field has-addons\">\n                <p class=\"control\">\n                  <input\n                    class=\"input\"\n                    type=\"text\"\n                    v-model=\"searchInput\"\n                    placeholder=\"Buscar Tarefa\"\n                  />\n                </p>\n                <p class=\"control\">\n                  <button class=\"button is-danger is-rounded\" @click=\"busca\">\n                    Pesquisar\n                  </button>\n                </p>\n              </div>\n            </div>\n          </div>\n        </nav>\n        <template>\n          <b-tabs>\n            <!-- <b-tab-item label=\"Table\" > -->\n            <b-table\n              :paginated=\"true\"\n              :data=\"answerList\"\n              :columns=\"columns\"\n              :selected.sync=\"selected\"\n              focusable\n            ></b-table>\n          </b-tabs>\n          <!-- <button class=\"button is-danger is-outlined is-rounded\">\n            Alterar Nota\n          </button> -->\n\n          <button\n            class=\"button is-danger is-rounded\"\n            @click=\"confirmDeleteNota(selected._id)\"\n          >\n            Excluir Nota\n          </button>\n        </template>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n\n\n<script>\nimport Navbar from \"@/components/Navbar.vue\";\n\nexport default {\n  name: \"Dashboard\",\n  components: {\n    Navbar,\n  },\n\n  data() {\n    const data = [];\n\n    return {\n      productList: [],\n      answerList: [],\n      showProductAddModal: false,\n      errorName: false,\n      form: {\n        name: \"\",\n        description: \"\",\n      },\n      data,\n      selected: data[1],\n      columns: [\n        {\n          field: \"card\",\n          label: \"Cartão\",\n          width: \"80\",\n        },\n        {\n          field: \"produto\",\n          label: \"Produto\",\n        },\n        {\n          field: \"dev\",\n          label: \"Desenvolvedor\",\n        },\n        {\n          field: \"qa\",\n          label: \"QA\",\n        },\n        {\n          field: \"data\",\n          label: \"Data\",\n          centered: true,\n        },\n        {\n          field: \"nota\",\n          label: \"Nota\",\n        },\n        {\n          field: \"comentario\",\n          label: \"Comentário\",\n        },\n      ],\n    };\n  },\n\n  methods: {\n    busca() {\n      if (this.searchInput != \"\") {\n        this.answerList = this.answerList.filter(\n          (answer) => answer.card === this.searchInput\n        );\n      } else {\n        this.list_answer();\n      }\n      this.searchInput = \"\";\n    },\n\n    alertCustom() {\n      this.$buefy.dialog.alert({\n        title: \"Title Alert\",\n        message: \"I have a title, a custom button and <b>HTML</b>!\",\n        confirmText: \"Cool!\",\n      });\n    },\n\n    confirmCustomDelete(productId) {\n      this.$buefy.dialog.confirm({\n        title: \"Deletar Sistema\",\n        message:\n          \"Deseja excluir o sistema? <br> Esta ação não poderá ser desfeita.\",\n        confirmText: \"Excluir\",\n        type: \"is-danger\",\n        onConfirm: () => {\n          this.remove(productId);\n          this.successDelete();\n        },\n      });\n    },\n\n    confirmDeleteNota(answerId) {\n      this.$buefy.dialog.confirm({\n        title: \"Deletar Nota\",\n        message:\n          \"Deseja excluir esta nota? <br> Esta ação não poderá ser desfeita.\",\n        confirmText: \"Excluir\",\n        type: \"is-danger\",\n        onConfirm: () => {\n          this.removeNota(answerId);\n          this.successDelete();\n        },\n      });\n    },\n\n    successDelete() {\n      this.$buefy.toast.open({\n        message: \"Exclusão efetuada com sucesso.\",\n        type: \"is-success\",\n      });\n    },\n\n    closeModal() {\n      this.showProductAddModal = false;\n    },\n\n    create() {\n      this.errorName = false;\n\n      if (this.form.name === \"\") {\n        this.errorName = true;\n        return null;\n      }\n\n      window.axios.post(\"/products\", this.form).then(async (res) => {\n        await res.data;\n      });\n\n      this.form.name = \"\";\n      this.form.description = \"\";\n      this.list();\n    },\n\n    remove(productId) {\n      window.axios.delete(\"/products/\" + productId).then(async (res) => {\n        await res.data;\n        this.list();\n      });\n    },\n\n    removeNota(answerId) {\n      window.axios.delete(\"/answers/\" + answerId).then(async (res) => {\n        await res.data;\n        this.list_answer();\n      });\n    },\n\n    list_answer() {\n      window.axios.get(\"/answers\").then(async (res) => {\n        this.answerList = await res.data;\n      });\n    },\n\n    list() {\n      window.axios.get(\"/products\").then(async (res) => {\n        this.productList = await res.data;\n        this.showProductAddModal = false;\n      });\n    },\n  },\n\n  mounted() {\n    this.list();\n    this.list_answer();\n    this.showProductAddModal = false;\n  },\n};\n</script>\n\n"],"sourceRoot":"src/views"}]}